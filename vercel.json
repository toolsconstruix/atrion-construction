{
  "buildCommand": "npm run build",
  "redirects": [
    {
      "source": "/cleaningservices/:path*",
      "destination": "https://cleaningservices.atrionconstruction.com/:path*",
      "permanent": true
    },
    {
      "source": "/cleaningservices",
      "destination": "https://cleaningservices.atrionconstruction.com/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:path((?!cleaningservices).*)*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
