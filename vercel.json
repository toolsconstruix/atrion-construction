{
  "buildCommand": "npm run build",
  "rewrites": [
    {
      "source": "/cleaningservices/:path*",
      "destination": "https://cleaningservices.atrionconstruction.com/:path*"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
